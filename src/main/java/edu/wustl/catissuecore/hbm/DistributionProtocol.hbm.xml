<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="edu.wustl.catissuecore.domain.DistributionProtocol" table="CATISSUE_DISTRIBUTION_PROTOCOL" lazy="true">

    <cache usage="read-write" />

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_DISTRI_PROT_SEQ</param>
            </generator>
        </id>

        <many-to-one
            name="principalInvestigator"
            class="edu.wustl.catissuecore.domain.User"
            cascade="none"

            update="true"
            insert="true"
            column="PRINCIPAL_INVESTIGATOR_ID"
            lazy="false"
        />

        <property
            name="title"
            type="string"
            update="true"
            insert="true"
            column="TITLE"
            length="255"
            not-null="true"
            unique="true"
        />

        <property
            name="shortTitle"
            type="string"
            update="true"
            insert="true"
            column="SHORT_TITLE"
            length="255"
        />

        <property
            name="irbIdentifier"
            type="string"
            update="true"
            insert="true"
            column="IRB_IDENTIFIER"
            length="255"
        />

        <property
            name="startDate"
            type="date"
            update="true"
            insert="true"
            column="START_DATE"
            length="50"
        />

        <property
            name="endDate"
            type="date"
            update="true"
            insert="true"
            column="END_DATE"
            length="50"
        />

        <property
            name="enrollment"
            type="int"
            update="true"
            insert="true"
            column="ENROLLMENT"
            length="50"
        />

        <property
            name="descriptionURL"
            type="string"
            update="true"
            insert="true"
            column="DESCRIPTION_URL"
            length="255"
        />

        <property
            name="activityStatus"
            type="string"
            update="true"
            insert="true"
            column="ACTIVITY_STATUS"
            length="50"
        />

        <set
            name="distributionSpecimenRequirementCollection"
            table="CATISSUE_DISTRIBUTION_SPEC_REQ"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
            fetch="join"
            lazy="false"
        >

        <cache usage="read-write" />

            <key
                column="DISTRIBUTION_PROTOCOL_ID"
            >
            </key>

            <one-to-many
                class="edu.wustl.catissuecore.domain.DistributionSpecimenRequirement"
             />

        </set>

        <set
            name="collectionProtocolCollection"
            table="CATISSUE_COLL_DISTRIBUTION_REL"
            lazy="true"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

        <cache usage="read-write" />

            <key
                column="DISTRIBUTION_PROTOCOL_ID"
            >
            </key>

            <many-to-many
                class="edu.wustl.catissuecore.domain.CollectionProtocol"
                column="COLLECTION_PROTOCOL_ID"

             />

        </set>

    </class>
</hibernate-mapping>
